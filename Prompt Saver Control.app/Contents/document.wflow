					<key>COMMAND_STRING</key>
					<string>#!/bin/zsh
set -euo pipefail
set --

REPO_DIR="/Users/seanengland/dev/prompt-saver-tauri-v2-scaffold"
cd "$REPO_DIR"

# Basic sanity
if [[ ! -f "src-tauri/tauri.conf.json" ]]; then
  /usr/bin/osascript -e 'display dialog "Cannot find src-tauri/tauri.conf.json. Check REPO_DIR in the Automator script." buttons {"OK"}'
  exit 1
fi

# Choose action
ACTION=$(/usr/bin/osascript &lt;&lt;EOF
tell application "System Events"
  button returned of (display dialog "What do you want to do?" buttons {"Cancel", "Push only", "Push + Release"} default button "Push only")
end tell
EOF
)
[[ "$ACTION" == "Cancel" ]] &amp;&amp; exit 0

# Exit early if nothing changed
if [[ -z "$(git status --porcelain)" ]]; then
  /usr/bin/osascript -e 'display dialog "No changes to commit." buttons {"OK"}'
  exit 0
fi

# Read version when releasing
VERSION=""
if [[ "$ACTION" == "Push + Release" ]]; then
  VERSION=$(python3 - &lt;&lt;'PY'
import json
with open("src-tauri/tauri.conf.json","r",encoding="utf-8") as f:
    data=json.load(f)
print(data.get("version",""))
PY
)
  if [[ -z "$VERSION" ]]; then
    /usr/bin/osascript -e 'display dialog "Could not read version from src-tauri/tauri.conf.json" buttons {"OK"}'
    exit 1
  fi
fi

# Auto commit message
# - Release: "release: vX.Y.Z"
# - Push only: "chore: update &lt;N&gt; file(s)"
CHANGED_COUNT=$(git status --porcelain | wc -l | tr -d ' ')
if [[ "$ACTION" == "Push + Release" ]]; then
  COMMIT_MSG="release: v$VERSION"
else
  COMMIT_MSG="chore: update $CHANGED_COUNT file(s)"
fi

# Commit + push
git add -A
git commit -m "$COMMIT_MSG"
git push

# Tag + push tag + create GitHub Release (auto-generated notes) if requested
if [[ "$ACTION" == "Push + Release" ]]; then
  TAG="v$VERSION"

  if git rev-parse -q --verify "refs/tags/$TAG" &gt;/dev/null; then
    /usr/bin/osascript -e 'display dialog "Tag already exists: '"$TAG"'\n\nBump version first." buttons {"OK"}'
    exit 1
  fi

  git tag -a "$TAG" -m "Prompt Saver $TAG"
  git push origin "$TAG"

  # Create GitHub Release with auto-generated notes (Option 2)
  if command -v gh &gt;/dev/null 2&gt;&amp;1; then
    if ! gh auth status &gt;/dev/null 2&gt;&amp;1; then
      /usr/bin/osascript -e 'display dialog "GitHub CLI (gh) is installed but not authenticated.\n\nRun in Terminal:\n\n  gh auth login\n\nThen try again." buttons {"OK"}'
      exit 1
    fi

    # Fail fast if release already exists
    if gh release view "$TAG" &gt;/dev/null 2&gt;&amp;1; then
      /usr/bin/osascript -e 'display dialog "GitHub Release already exists for '"$TAG"'.\n\nTag was pushed; release was not recreated." buttons {"OK"}'
      exit 1
    fi

    gh release create "$TAG" \
      --title "Prompt Saver $TAG" \
      --generate-notes

  else
    /usr/bin/osascript -e 'display dialog "Tag pushed: '"$TAG"'.\n\nTo auto-create a GitHub Release with generated notes, install GitHub CLI:\n\n  brew install gh" buttons {"OK"}'
  fi
fi

/usr/bin/osascript -e 'display dialog "âœ… Done!" buttons {"OK"}'</string>
